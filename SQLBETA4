ALTER TABLE  `LINEASCOMPRA` DROP FOREIGN KEY  `LINEA>FACTURA` ;

ALTER TABLE  `LINEASCOMPRA` ADD CONSTRAINT  `LINEA>FACTURA` FOREIGN KEY (  `idFactura` ) REFERENCES  `BussinessControl`.`FACTURAS` (
`id`
) ON DELETE CASCADE ON UPDATE CASCADE ;

ALTER TABLE  `LINEASPRESUPUESTOS` DROP FOREIGN KEY  `LNPRESUPUESTO > PRESUPUESTO` ;

ALTER TABLE  `LINEASPRESUPUESTOS` ADD CONSTRAINT  `LNPRESUPUESTO > PRESUPUESTO` FOREIGN KEY (  `idFactura` ) REFERENCES  `BussinessControl`.`PRESUPUESTOS` (
`ID`
) ON DELETE CASCADE ON UPDATE CASCADE ;
ALTER TABLE  `AJUSTES` ADD  `IRPF` FLOAT( 10 ) NOT NULL AFTER  `RecargoEquivalencia` ;